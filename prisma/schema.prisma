generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model categories {
  id       Int    @id @default(autoincrement())
  category String @unique
}

model images {
  id        Int    @id @default(autoincrement())
  productId Int
  url       String
}

model products {
  id          Int    @id @default(autoincrement())
  title       String
  description String
  price       Int
  categoryId  Int
}